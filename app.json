{
  "expo": {
    "name": "Doc-Vault",
    "slug": "doc-vault",
    "owner": "amuhammad8169",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "docvault",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.docvault.app",
      "infoPlist": {
        "NSFaceIDUsageDescription": "Allow Doc Vault to use Face ID for secure unlocking"
      }
    },
    "android": {
      "package": "com.docvault.app",
      "permissions": [
        "android.permission.USE_BIOMETRIC",
        "android.permission.USE_FINGERPRINT"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-local-authentication",
      "@react-native-google-signin/google-signin",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#00796B"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "397d7c0a-4b29-415c-9590-6ce80cb90717"
      }
    }
  }
}
